@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ex: <http://www.example.com#> .

@prefix apple: <http://my-data.org/data-dumps/apple#> .

apple:AppleKnowledgeEvent a rdfs:Class ;
    rdfs:label "Apple KnowledgeC Event" ;
    rdfs:comment "An event generated by an Apple device" .

apple:uuid a rdf:Property ;
    rdfs:label "uuid" ;
    rdfs:comment "The unique identifier of the event" ;
    rdfs:domain apple:AppleKnowledgeEvent ;
    rdfs:range xsd:string .

apple:bundleId a rdf:Property ;
    rdfs:label "bundleId" ;
    rdfs:comment "The bundle identifier of the app that generated the event" ;
    rdfs:domain apple:AppleKnowledgeEvent ;
    rdfs:range xsd:string .

apple:deviceId a rdf:Property ;
    rdfs:label "deviceId" ;
    rdfs:comment "The unique identifier of the device that generated the event" ;
    rdfs:domain apple:AppleKnowledgeEvent ;
    rdfs:range xsd:string .

apple:startTimestamp a rdf:Property ;
    rdfs:label "startTimestamp" ;
    rdfs:comment "The timestamp of the event" ;
    rdfs:domain apple:AppleKnowledgeEvent ;
    rdfs:range xsd:dateTime .